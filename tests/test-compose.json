{
    "default-manifest": "test.mpp.yml",
    "distro": ["autosd9-latest-sig", "autosd9"],
    "skip-distro": ["rhivos", "rhivos1"],
    "arch": [ "x86_64", "aarch64" ],
    "mode": [ "image", "package" ],
    "target": [ "qemu",
                {
                    "value": "ridesx4",
                    "arch-is": "aarch64",
                    "distro-is-not": "eln"
                },
                {
                    "value": "rcar_s4",
                    "arch-is": "aarch64"
                },
                {
                    "value": "rpi4",
                    "arch-is": "aarch64"
                },
                {
                    "value": "abootqemu",
                    "arch-is": "aarch64",
                    "distro-is": ["autosd9-latest-sig"]
                }
              ],
    "defines": [
        "",
        { "value": "use_qm=true",
          "distro-is-not": ["eln", "autosd10-latest-sig"]}
    ]
}
